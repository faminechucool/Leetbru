class Solution {
    public int[][] transpose(int[][] matrix) {
        //row
        int lenrow=matrix.length;
        //column
        int lencolumn=matrix[0].length;
        int[][]trans=new int[lencolumn][lenrow];
        for(int i=0;i<matrix.length;i++){
            for(int j=0;j<matrix[i].length;j++){
                trans[j][i]=matrix[i][j];
            }
        }
       return trans; 
    }
}
